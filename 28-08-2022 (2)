#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

   
    char s[1000];
    gets(s);
    int m[10]={0};
    int i;
    for(i=0;s[i]!='\0';i++)
    {
    	if(s[i]>='0'&&s[i]<='9')
    	{
    		int val=s[i]-'0';
    		m[val]=m[val]+1;
		}
	}
	for(i=0;i<10;i++)
	{
		printf("%d ",m[i]);
	}
	return 0;
}
	
Output:
e1122237hddvvjb9
0 2 3 1 0 0 0 1 0 1
